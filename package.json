    "build": "node scripts/build.js",
    "ci": "yarn test:unit && yarn coverage",
    "clean": "rimraf build",
    "coverage": "codecov",
    "deploy": "gh-pages -d build",
    "dev": "node scripts/start.js",
    "format": "prettier --write '{,!(node_modules)/}**/*.{js,jsx,ts,tsx,json}'",
    "lint:css": "stylelint --config .stylelintrc.json --formatter verbose './src/**/*.{css,js,jsx,ts,tsx}'",
    "lint:ts": "tslint --project .",
    "ncu": "ncu",
    "ncuu": "ncu --upgrade",
    "nuke": "rimraf node_modules && rm yarn.lock",
    "predeploy": "yarn build",
    "test": "jest --verbose",
    "test:dev": "concurrently \"yarn dev\" \"yarn test\"",
    "test:unit": "jest --testPathIgnorePatterns visual_regressions --verbose",
    "static": "chromium-browser build/index.html"
  },
